<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/judul_profile_target"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat"
        android:text="Judul Target"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="#0A0B0B"
        tools:ignore="MissingConstraints"
        android:textStyle="bold"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/deskripsi_target_profile"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:ellipsize="end"
        android:fontFamily="@font/montserrat"
        android:maxLines="1"
        android:text="deskripsi as dnas nais nas"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@id/judul_profile_target"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>